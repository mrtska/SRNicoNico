    <SolidColorBrush x:Key="BorderBrushKey" Color="#FF3F3F46" />
    <SolidColorBrush x:Key="SemiactiveBackgroundBrushKey" Color="#FF333337" />
    <SolidColorBrush x:Key="SemiactiveBorderBrushKey" Color="#FF3F3F46" />
